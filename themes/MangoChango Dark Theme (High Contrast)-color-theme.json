{
  "name": "MangoChango Dark Theme (High Contrast)",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#FFFFFF",
    "editorCursor.foreground": "#FF9900",
    "editor.lineHighlightBackground": "#333333",
    "editor.selectionBackground": "#FF990080",
    "editor.inactiveSelectionBackground": "#FF990040",
    "editorIndentGuide.background1": "#444444",
    "editorIndentGuide.activeBackground1": "#FF9900",
    "editorWhitespace.foreground": "#444444",
    "editor.selectionHighlightBackground": "#FF990080",
    "activityBar.background": "#e26944",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#CCCCCC",
    "statusBar.background": "#413875",
    "statusBar.foreground": "#FFFFFF",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#FFFFFF",
    "button.background": "#FF9900",
    "button.foreground": "#FFFFFF",
    "input.background": "#333333",
    "input.foreground": "#FFFFFF",
    "input.border": "#FF9900",
    "dropdown.background": "#333333",
    "dropdown.foreground": "#FFFFFF",
    "dropdown.border": "#FF9900",
    "list.activeSelectionBackground": "#FF9900",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.hoverBackground": "#FF990080",
    "list.hoverForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#FF990040",
    "list.inactiveSelectionForeground": "#FFFFFF",
    "badge.background": "#FF9900",
    "badge.foreground": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#757575"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF5722"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#FF9900"
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#FF9900"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#FFCC80"
      }
    },
    {
      "scope": ["variable", "entity.name.variable", "variable.parameter"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#FFC107"
      }
    },
    {
      "scope": ["support.function", "support.constant", "support.variable"],
      "settings": {
        "foreground": "#FF9900"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#FF0000"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.sass",
        "entity.other.attribute-name.class.scss",
        "entity.other.attribute-name.class.less"
      ],
      "settings": {
        "foreground": "#FFCC80"
      }
    },
    {
      "scope": [
        "meta.selector",
        "entity.name.tag",
        "entity.name.tag.css",
        "entity.name.tag.sass",
        "entity.name.tag.scss",
        "entity.name.tag.less",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.sass",
        "entity.other.attribute-name.id.scss",
        "entity.other.attribute-name.id.less"
      ],
      "settings": {
        "foreground": "#FF9900"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.ts",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#FFCC80"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.ts",
        "entity.name.type.tsx",
        "entity.name.type.interface",
        "entity.name.type.interface.ts",
        "entity.name.type.interface.tsx",
        "entity.name.type.class",
        "entity.name.type.class.ts",
        "entity.name.type.class.tsx",
        "entity.name.type.enum",
        "entity.name.type.enum.ts",
        "entity.name.type.enum.tsx",
        "entity.name.type.function",
        "entity.name.type.function.ts",
        "entity.name.type.function.tsx",
        "entity.name.type.method",
        "entity.name.type.method.ts",
        "entity.name.type.method.tsx",
        "entity.name.type.property",
        "entity.name.type.property.ts",
        "entity.name.type.property.tsx"
      ],
      "settings": {
        "foreground": "#5499e9"
      }
    },
    {
      "scope": [
        "support.type.property-name.json",
        "variable.other.property.json"
      ],
      "settings": {
        "foreground": "#8bc9e7"
      }
    },
    {
      "scope": ["constant.string.json"],
      "settings": {
        "foreground": "#07ff51"
      }
    },
    {
      "scope": ["string.quoted.double.json", "string.quoted.single.json"],
      "settings": {
        "foreground": "#9CDBB3"
      }
    },
    {
      "scope": ["constant.numeric.json", "constant.language.boolean.json"],
      "settings": {
        "foreground": "#FF5722"
      }
    },
    {
      "scope": [
        "punctuation.separator.key-value.json",
        "punctuation.separator.array.json"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "JavaScript string interpolation ${}",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
        "punctuation.definition.template-expression.end.js"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    }
  ]
}
